{
  "applicationId": "js.giveaway.otron",
  "defaultMode": "window",
  "version": "1.12.0",
  "port": 0,
  "url": "/resources/",
  "enableServer": true,
  "enableNativeAPI": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": ["app.*", "os.*", "filesystem.*", "storage.*", "window.*", "clipboard.*", "debug.log"],
  "globalVariables": {
    "TS": "MDZ6dGY1amYwdzd1dDFzaWF1c3hnOHFnZmx2ZGJm",
    "TID": "aWQ0Zm9qbzQ1NWoxbjJybGR6ODgycmV0dHZ1aHl1",
    "APP_VERSION": "1.12.0"
  },
  "modes": {
    "window": {
      "title": "Giveaway-o-tron",
      "width": 800,
      "height": 600,
      "minWidth": 700,
      "minHeight": 400,
      "fullScreen": false,
      "alwaysOnTop": true,
      "enableInspector": true,
      "icon": "/resources/icons/appIcon.png",
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    },
    "browser": {
      "globalVariables": {
        "TS": "MDZ6dGY1amYwdzd1dDFzaWF1c3hnOHFnZmx2ZGJm",
        "TID": "aWQ0Zm9qbzQ1NWoxbjJybGR6ODgycmV0dHZ1aHl1",
        "APP_VERSION": "1.12.0"
      },
      "nativeBlockList": ["filesystem.*"]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": ["app.*"]
    }
  },
  "cli": {
    "binaryName": "giveaway-o-tron",
    "resourcesPath": "/resources/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.3.0"
  }
}
